{
  "rules": {
    "indent": [
      2,
      2,
      {
        "SwitchCase": 1
      }
    ],
    "semi": [
      2,
      "always"
    ],
    "quotes": [
      2,
      "single"
    ],
    "no-console": 1,
    "eqeqeq": 1,
    "strict": 0
  },
  "parser": "babel-eslint",
  "ecmaFeatures": {
    "modules": true,
    "classes": true
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "mocha": true
  },
  "extends": "eslint:recommended",
  "globals": {
    "cc": true,
    "ccui": true,
    "ccs": true,
    "io": true,
    "assert": true,
    "log": true
  }
}